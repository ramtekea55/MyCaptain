#include<stdio.h>
int main()
{
    int n;
    printf("What is your choice: ");
    scanf( "%d", &n);
    switch(n)
    {
        case 1: printf("Food Item: Sandwich\n");
        printf("Price: Rs149\n");
        break;
        case 2: printf("Food Item: French Fries\n");
        printf("Price: Rs99\n");
        break;
        case 3: printf("Food Item: Pasta\n");
        printf("Price: Rs179\n");
        break;
        case 4: printf("Food Item: Burger\n");
        printf("Price: Rs129\n");
        break;
        case 5: printf("Food Item: Pizza\n");
        printf("Price: Rs239\n");
        break;
        default: printf("Enter Correct Order Number");
    }
    return 0;
}
