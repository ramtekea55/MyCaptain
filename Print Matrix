#include<stdio.h>
main()
{
 int matrix[3][3];
 int i,j;
 printf("Input Elements in the matrix: ");
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("\nElement-[%d],[%d] :",i,j);
   scanf("%d",&matrix[i][j]);
  }
 }
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("%d ",matrix[i][j]);
  }
 printf("\n");
 }
return 0;
}
