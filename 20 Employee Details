#include<stdio.h>
struct employee
{
 char name[7];
 int age;
 int phno;
 int sal;
};
main()
{
 struct employee E[2];
 int i;
 for(i=0;i<20;i++)
 { 
  printf("Enter %d employee's details \n",i+1);
  printf("Enter employee name: \n");
  scanf("%s",E[i].name);
  printf("Enter employee age: \n");
  scanf("%d",&E[i].age);
  printf("Enter employee phone number: \n");
  scanf("%d",&E[i].phno);
  printf("Enter employee salary: \n");
  scanf("%d",&E[i].sal);
 }
 printf("Name   ");
 printf("Age ");
 printf("Phone     ");
 printf("Salary \n");
 for(i=0;i<20;i++)
 {
  printf("%s ",E[i].name);
  printf("%d ",E[i].age);
  printf("%d ",E[i].phno);
  printf("%d ",E[i].sal);
  printf("\n");
 }
return 0;
}
